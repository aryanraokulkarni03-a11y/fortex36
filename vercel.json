{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "graphrag/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/graph/(.*)",
            "dest": "graphrag/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}